	local script = Instance.new('LocalScript', TextButton)

	local player = game.Players.LocalPlayer
	
	script.Parent.MouseButton1Click:Connect(function()
		player.PlayerGui.ScreenGui.ScriptsFrame.Visible = false
	end)
end
coroutine.wrap(AEHYP_fake_script)()
local function RLFUKVN_fake_script() -- ScriptsFrame.LocalScript 
	local script = Instance.new('LocalScript', ScriptsFrame)

	script.Parent.Active = true
	script.Parent.Draggable= true
end
coroutine.wrap(RLFUKVN_fake_script)()
